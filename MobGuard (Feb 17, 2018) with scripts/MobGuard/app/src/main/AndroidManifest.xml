<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chrisjerico.jubecermobileapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/csgms"
        android:label="MOBguard"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginClient" />
        <activity android:name=".LoginSg" />
        <activity android:name=".Profile" />
        <activity android:name=".ProfileSg" />
        <activity android:name=".MainProfile" />
        <activity android:name=".MainProfileSg" />
        <activity android:name=".ClientEstablishment" />
        <activity android:name=".ClientMessage" />
        <activity android:name=".ClientRequest" />
        <activity android:name=".ClientAddGuard" />
        <activity android:name=".GuardAnnouncement" />
        <activity android:name=".GuardClient" />
        <activity android:name=".GuardMessage" />
        <activity android:name=".GuardRequest" />
        <activity android:name=".GuardServiceReq" />
    </application>

</manifest>